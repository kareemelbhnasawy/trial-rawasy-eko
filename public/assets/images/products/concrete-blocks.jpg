<!-- Concrete Blocks Product Image -->
<svg width="300" height="400" viewBox="0 0 300 400" fill="none" xmlns="http://www.w3.org/2000/svg">
  <rect width="300" height="400" fill="#f5f5f5"/>
  
  <!-- Stack of concrete blocks -->
  <g>
    <!-- Bottom row -->
    <rect x="40" y="300" width="60" height="40" fill="#D3D3D3" stroke="#999999" stroke-width="1"/>
    <rect x="100" y="300" width="60" height="40" fill="#D3D3D3" stroke="#999999" stroke-width="1"/>
    <rect x="160" y="300" width="60" height="40" fill="#D3D3D3" stroke="#999999" stroke-width="1"/>
    
    <!-- Middle row -->
    <rect x="70" y="260" width="60" height="40" fill="#DCDCDC" stroke="#999999" stroke-width="1"/>
    <rect x="130" y="260" width="60" height="40" fill="#DCDCDC" stroke="#999999" stroke-width="1"/>
    
    <!-- Top row -->
    <rect x="40" y="220" width="60" height="40" fill="#E5E5E5" stroke="#999999" stroke-width="1"/>
    <rect x="100" y="220" width="60" height="40" fill="#E5E5E5" stroke="#999999" stroke-width="1"/>
    <rect x="160" y="220" width="60" height="40" fill="#E5E5E5" stroke="#999999" stroke-width="1"/>
    
    <!-- Block holes/cavities -->
    <rect x="52" y="232" width="12" height="16" fill="#BEBEBE"/>
    <rect x="76" y="232" width="12" height="16" fill="#BEBEBE"/>
    <rect x="112" y="232" width="12" height="16" fill="#BEBEBE"/>
    <rect x="136" y="232" width="12" height="16" fill="#BEBEBE"/>
    <rect x="172" y="232" width="12" height="16" fill="#BEBEBE"/>
    <rect x="196" y="232" width="12" height="16" fill="#BEBEBE"/>
  </g>
  
  <!-- Product info box -->
  <rect x="30" y="80" width="240" height="120" fill="white" stroke="#ddd" stroke-width="2" rx="8"/>
  
  <text x="150" y="105" font-family="Arial" font-size="18" font-weight="bold" fill="#2C3C28" text-anchor="middle">Concrete Blocks</text>
  <text x="150" y="125" font-family="Arial" font-size="14" fill="#629D23" text-anchor="middle">8" x 8" x 16" Standard</text>
  
  <text x="45" y="145" font-family="Arial" font-size="11" fill="#333">• ASTM C90 Compliant</text>
  <text x="45" y="160" font-family="Arial" font-size="11" fill="#333">• 1900 psi minimum strength</text>
  <text x="45" y="175" font-family="Arial" font-size="11" fill="#333">• Weight: 38-40 lbs each</text>
  <text x="45" y="190" font-family="Arial" font-size="11" fill="#333">• Durable concrete construction</text>
  
  <!-- Price badge -->
  <circle cx="60" cy="50" r="25" fill="#FFE884" stroke="#2C3C28" stroke-width="2"/>
  <text x="60" y="47" font-family="Arial" font-size="10" font-weight="bold" fill="#2C3C28" text-anchor="middle">$2.25</text>
  <text x="60" y="57" font-family="Arial" font-size="8" fill="#2C3C28" text-anchor="middle">per block</text>
  
  <!-- Brand -->
  <rect x="200" y="35" width="60" height="20" fill="#2C3C28" rx="3"/>
  <text x="230" y="47" font-family="Arial" font-size="10" font-weight="bold" fill="white" text-anchor="middle">OLDCASTLE</text>
  
  <!-- Dimensions indicator -->
  <text x="150" y="370" font-family="Arial" font-size="12" fill="#666" text-anchor="middle">Standard 8" x 8" x 16" CMU</text>
</svg>
