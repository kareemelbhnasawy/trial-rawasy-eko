<!-- Excavator Product Image -->
<svg width="300" height="400" viewBox="0 0 300 400" fill="none" xmlns="http://www.w3.org/2000/svg">
  <rect width="300" height="400" fill="#87CEEB"/>
  
  <!-- Ground -->
  <rect x="0" y="300" width="300" height="100" fill="#8B7355"/>
  <rect x="0" y="290" width="300" height="20" fill="#9ACD32"/>
  
  <!-- Excavator body -->
  <g>
    <!-- Tracks -->
    <ellipse cx="120" cy="280" rx="40" ry="15" fill="#2C3C28"/>
    <ellipse cx="180" cy="280" rx="40" ry="15" fill="#2C3C28"/>
    
    <!-- Track details -->
    <rect x="90" y="275" width="60" height="10" fill="#4A4A4A"/>
    <rect x="150" y="275" width="60" height="10" fill="#4A4A4A"/>
    
    <!-- Main body -->
    <rect x="130" y="220" width="70" height="60" rx="10" fill="#FFE884"/>
    <rect x="135" y="225" width="60" height="50" rx="8" fill="#FDD835"/>
    
    <!-- Cabin -->
    <rect x="140" y="200" width="40" height="40" rx="5" fill="#FFE884"/>
    <rect x="145" y="205" width="30" height="30" rx="3" fill="#87CEEB" opacity="0.7"/>
    
    <!-- Counterweight -->
    <rect x="100" y="235" width="35" height="35" rx="5" fill="#FFE884"/>
    
    <!-- Boom (arm) -->
    <rect x="200" y="215" width="60" height="8" rx="4" fill="#2C3C28"/>
    
    <!-- Stick (forearm) -->
    <rect x="255" y="200" width="35" height="6" rx="3" fill="#2C3C28" transform="rotate(20 272 203)"/>
    
    <!-- Bucket -->
    <path d="M280 190 L295 200 L290 210 L275 200 Z" fill="#4A4A4A"/>
    <path d="M295 200 L300 205 L295 210 L290 205" fill="#666666"/>
    
    <!-- Hydraulic cylinders -->
    <rect x="190" y="218" width="15" height="4" rx="2" fill="#C0C0C0"/>
    <rect x="245" y="205" width="12" height="3" rx="1.5" fill="#C0C0C0"/>
    
    <!-- Cabin details -->
    <rect x="148" y="210" width="6" height="8" fill="#333333"/>
    <rect x="156" y="210" width="6" height="8" fill="#333333"/>
    <rect x="164" y="210" width="6" height="8" fill="#333333"/>
  </g>
  
  <!-- Product info overlay -->
  <rect x="20" y="30" width="260" height="140" fill="white" stroke="#ddd" stroke-width="2" rx="10" opacity="0.95"/>
  
  <text x="150" y="55" font-family="Arial" font-size="20" font-weight="bold" fill="#2C3C28" text-anchor="middle">Caterpillar 320 Excavator</text>
  <text x="150" y="75" font-family="Arial" font-size="14" fill="#629D23" text-anchor="middle">Daily Rental Available</text>
  
  <text x="40" y="100" font-family="Arial" font-size="11" fill="#333">• Operating Weight: 44,900 lbs</text>
  <text x="40" y="115" font-family="Arial" font-size="11" fill="#333">• Engine Power: 158 HP</text>
  <text x="40" y="130" font-family="Arial" font-size="11" fill="#333">• Bucket Capacity: 1.5 cubic yards</text>
  <text x="40" y="145" font-family="Arial" font-size="11" fill="#333">• Dig Depth: 21 feet 8 inches</text>
  <text x="40" y="160" font-family="Arial" font-size="11" fill="#333">• Maximum Reach: 31 feet 2 inches</text>
  
  <!-- Rental price -->
  <circle cx="80" cy="380" r="35" fill="#FF6B35"/>
  <text x="80" y="374" font-family="Arial" font-size="14" font-weight="bold" fill="white" text-anchor="middle">$850.00</text>
  <text x="80" y="388" font-family="Arial" font-size="10" fill="white" text-anchor="middle">per day</text>
  
  <!-- Brand logo -->
  <rect x="170" y="360" width="70" height="25" fill="#FFE884" stroke="#2C3C28" stroke-width="2" rx="5"/>
  <text x="205" y="375" font-family="Arial" font-size="12" font-weight="bold" fill="#2C3C28" text-anchor="middle">Caterpillar</text>
  
  <!-- EPA Tier 4 badge -->
  <rect x="250" y="360" width="40" height="25" fill="#629D23" rx="3"/>
  <text x="270" y="370" font-family="Arial" font-size="8" font-weight="bold" fill="white" text-anchor="middle">EPA</text>
  <text x="270" y="378" font-family="Arial" font-size="8" fill="white" text-anchor="middle">TIER 4</text>
  
  <!-- Delivery included badge -->
  <rect x="180" y="320" width="100" height="20" fill="#2C3C28" rx="10"/>
  <text x="230" y="332" font-family="Arial" font-size="10" font-weight="bold" fill="#FFE884" text-anchor="middle">Delivery Included</text>
</svg>
