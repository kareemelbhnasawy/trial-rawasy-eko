<!-- Electrical Wire Product Image -->
<svg width="300" height="400" viewBox="0 0 300 400" fill="none" xmlns="http://www.w3.org/2000/svg">
  <rect width="300" height="400" fill="#f9f9f9"/>
  
  <!-- Wire spool -->
  <g>
    <!-- Spool base -->
    <ellipse cx="150" cy="200" rx="90" ry="30" fill="#8B4513"/>
    <ellipse cx="150" cy="160" rx="90" ry="30" fill="#A0522D"/>
    
    <!-- Spool sides -->
    <rect x="60" y="160" width="180" height="40" fill="#654321"/>
    
    <!-- Wire coils -->
    <ellipse cx="150" cy="180" rx="70" ry="25" fill="#FF4500"/>
    <ellipse cx="150" cy="175" rx="65" ry="22" fill="#FF6347"/>
    <ellipse cx="150" cy="170" rx="60" ry="20" fill="#FF4500"/>
    <ellipse cx="150" cy="165" rx="55" ry="18" fill="#FF6347"/>
    
    <!-- Center hub -->
    <ellipse cx="150" cy="180" rx="20" ry="8" fill="#2C3C28"/>
    
    <!-- Wire end -->
    <path d="M90 165 Q70 160 50 170" stroke="#FF4500" stroke-width="4" fill="none"/>
    <circle cx="45" cy="175" r="3" fill="#FFD700"/>
  </g>
  
  <!-- Product label -->
  <rect x="30" y="40" width="240" height="100" fill="white" stroke="#ddd" stroke-width="2" rx="8"/>
  <text x="150" y="65" font-family="Arial" font-size="18" font-weight="bold" fill="#2C3C28" text-anchor="middle">Electrical Wire THHN</text>
  <text x="150" y="85" font-family="Arial" font-size="14" fill="#629D23" text-anchor="middle">12 AWG • 500 ft Roll</text>
  <text x="150" y="105" font-family="Arial" font-size="12" fill="#666" text-anchor="middle">THHN/THWN-2 Building Wire</text>
  <text x="150" y="120" font-family="Arial" font-size="10" fill="#666" text-anchor="middle">90°C Dry • 75°C Wet • 600V Rated</text>
  <text x="150" y="132" font-family="Arial" font-size="10" fill="#666" text-anchor="middle">UL Listed • NEC Compliant</text>
  
  <!-- Specifications -->
  <rect x="40" y="260" width="220" height="90" fill="#f8f9fa" stroke="#e5e5e5" stroke-width="1" rx="5"/>
  <text x="60" y="280" font-family="Arial" font-size="12" font-weight="bold" fill="#2C3C28">Wire Specifications:</text>
  <text x="60" y="295" font-family="Arial" font-size="10" fill="#333">• Gauge: 12 AWG (American Wire Gauge)</text>
  <text x="60" y="308" font-family="Arial" font-size="10" fill="#333">• Insulation: THHN/THWN-2 Nylon</text>
  <text x="60" y="321" font-family="Arial" font-size="10" fill="#333">• Temperature: 90°C dry, 75°C wet</text>
  <text x="60" y="334" font-family="Arial" font-size="10" fill="#333">• Voltage Rating: 600V</text>
  <text x="60" y="347" font-family="Arial" font-size="10" fill="#333">• Length: 500 feet per roll</text>
  
  <!-- Price -->
  <circle cx="80" cy="380" r="30" fill="#FF4500"/>
  <text x="80" y="376" font-family="Arial" font-size="12" font-weight="bold" fill="white" text-anchor="middle">$125.00</text>
  <text x="80" y="388" font-family="Arial" font-size="8" fill="white" text-anchor="middle">per roll</text>
  
  <!-- Brand -->
  <rect x="160" y="365" width="80" height="20" fill="#2C3C28" rx="3"/>
  <text x="200" y="377" font-family="Arial" font-size="11" font-weight="bold" fill="white" text-anchor="middle">Southwire</text>
  
  <!-- UL Listed badge -->
  <circle cx="240" cy="290" r="18" fill="#629D23"/>
  <text x="240" y="287" font-family="Arial" font-size="8" font-weight="bold" fill="white" text-anchor="middle">UL</text>
  <text x="240" y="296" font-family="Arial" font-size="6" fill="white" text-anchor="middle">LISTED</text>
  
  <!-- NEC badge -->
  <rect x="210" y="45" width="35" height="15" fill="#FFE884" stroke="#2C3C28" stroke-width="1" rx="3"/>
  <text x="227" y="54" font-family="Arial" font-size="8" font-weight="bold" fill="#2C3C28" text-anchor="middle">NEC</text>
  
  <!-- RoHS badge -->
  <rect x="250" y="45" width="25" height="15" fill="#629D23" rx="3"/>
  <text x="262" y="54" font-family="Arial" font-size="7" font-weight="bold" fill="white" text-anchor="middle">RoHS</text>
</svg>
