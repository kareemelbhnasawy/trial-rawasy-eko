<!-- Marine Plywood Product Image -->
<svg width="300" height="400" viewBox="0 0 300 400" fill="none" xmlns="http://www.w3.org/2000/svg">
  <rect width="300" height="400" fill="#f8f8f8"/>
  
  <!-- Plywood sheets stack -->
  <g>
    <!-- Main sheet -->
    <rect x="50" y="100" width="200" height="250" fill="#DEB887" stroke="#8B7355" stroke-width="2"/>
    
    <!-- Wood grain pattern -->
    <path d="M50 120 Q150 115 250 120" stroke="#CD853F" stroke-width="1" fill="none"/>
    <path d="M50 140 Q150 145 250 140" stroke="#CD853F" stroke-width="1" fill="none"/>
    <path d="M50 160 Q150 155 250 160" stroke="#CD853F" stroke-width="1" fill="none"/>
    <path d="M50 180 Q150 185 250 180" stroke="#CD853F" stroke-width="1" fill="none"/>
    <path d="M50 200 Q150 195 250 200" stroke="#CD853F" stroke-width="1" fill="none"/>
    <path d="M50 220 Q150 225 250 220" stroke="#CD853F" stroke-width="1" fill="none"/>
    <path d="M50 240 Q150 235 250 240" stroke="#CD853F" stroke-width="1" fill="none"/>
    <path d="M50 260 Q150 265 250 260" stroke="#CD853F" stroke-width="1" fill="none"/>
    <path d="M50 280 Q150 275 250 280" stroke="#CD853F" stroke-width="1" fill="none"/>
    <path d="M50 300 Q150 305 250 300" stroke="#CD853F" stroke-width="1" fill="none"/>
    <path d="M50 320 Q150 315 250 320" stroke="#CD853F" stroke-width="1" fill="none"/>
    
    <!-- Second sheet (slightly offset) -->
    <rect x="55" y="105" width="200" height="250" fill="#DEB887" stroke="#8B7355" stroke-width="1" opacity="0.8"/>
    
    <!-- Third sheet -->
    <rect x="60" y="110" width="200" height="250" fill="#DEB887" stroke="#8B7355" stroke-width="1" opacity="0.6"/>
  </g>
  
  <!-- Product label -->
  <rect x="40" y="30" width="220" height="60" fill="white" stroke="#ddd" stroke-width="2" rx="5"/>
  <text x="150" y="50" font-family="Arial" font-size="16" font-weight="bold" fill="#2C3C28" text-anchor="middle">Marine Grade Plywood</text>
  <text x="150" y="68" font-family="Arial" font-size="12" fill="#629D23" text-anchor="middle">1/2" x 4' x 8' Sheet</text>
  <text x="150" y="82" font-family="Arial" font-size="10" fill="#666" text-anchor="middle">Waterproof Phenolic Resin</text>
  
  <!-- Specifications on the side -->
  <rect x="260" y="120" width="35" height="100" fill="#FFE884" stroke="#2C3C28" stroke-width="1" rx="3"/>
  <text x="277" y="135" font-family="Arial" font-size="8" font-weight="bold" fill="#2C3C28" text-anchor="middle">APA</text>
  <text x="277" y="145" font-family="Arial" font-size="8" fill="#2C3C28" text-anchor="middle">PS1-19</text>
  <text x="277" y="160" font-family="Arial" font-size="8" font-weight="bold" fill="#2C3C28" text-anchor="middle">MARINE</text>
  <text x="277" y="170" font-family="Arial" font-size="8" fill="#2C3C28" text-anchor="middle">GRADE</text>
  <text x="277" y="190" font-family="Arial" font-size="8" fill="#2C3C28" text-anchor="middle">1/2"</text>
  <text x="277" y="200" font-family="Arial" font-size="8" fill="#2C3C28" text-anchor="middle">THICK</text>
  
  <!-- Price -->
  <circle cx="80" cy="380" r="30" fill="#2C3C28"/>
  <text x="80" y="376" font-family="Arial" font-size="12" font-weight="bold" fill="white" text-anchor="middle">$89.99</text>
  <text x="80" y="388" font-family="Arial" font-size="8" fill="white" text-anchor="middle">per sheet</text>
  
  <!-- Brand -->
  <rect x="150" y="365" width="80" height="18" fill="#629D23" rx="3"/>
  <text x="190" y="376" font-family="Arial" font-size="10" font-weight="bold" fill="white" text-anchor="middle">Georgia-Pacific</text>
  
  <!-- Quality stamps -->
  <circle cx="230" cy="380" r="15" fill="#FFE884" stroke="#2C3C28" stroke-width="1"/>
  <text x="230" y="383" font-family="Arial" font-size="8" font-weight="bold" fill="#2C3C28" text-anchor="middle">H2O</text>
  <text x="230" y="390" font-family="Arial" font-size="6" fill="#2C3C28" text-anchor="middle">PROOF</text>
</svg>
