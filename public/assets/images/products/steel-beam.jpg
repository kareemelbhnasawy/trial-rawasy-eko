<!-- Structural Steel Beam Product Image -->
<svg width="300" height="400" viewBox="0 0 300 400" fill="none" xmlns="http://www.w3.org/2000/svg">
  <rect width="300" height="400" fill="#f0f0f0"/>
  
  <!-- Steel beams stack -->
  <g>
    <!-- Main I-beam cross section view -->
    <rect x="80" y="120" width="140" height="160" fill="#C0C0C0"/>
    
    <!-- I-beam shape -->
    <!-- Top flange -->
    <rect x="70" y="120" width="160" height="25" fill="#B0B0B0"/>
    <rect x="75" y="125" width="150" height="15" fill="#A8A8A8"/>
    
    <!-- Web (middle) -->
    <rect x="140" y="145" width="20" height="110" fill="#989898"/>
    <rect x="142" y="147" width="16" height="106" fill="#909090"/>
    
    <!-- Bottom flange -->
    <rect x="70" y="255" width="160" height="25" fill="#B0B0B0"/>
    <rect x="75" y="260" width="150" height="15" fill="#A8A8A8"/>
    
    <!-- Side view of stacked beams -->
    <rect x="50" y="300" width="200" height="12" fill="#888888"/>
    <rect x="52" y="312" width="196" height="12" fill="#7A7A7A"/>
    <rect x="54" y="324" width="192" height="12" fill="#888888"/>
    
    <!-- Beam marking/stamping -->
    <text x="150" y="200" font-family="Arial" font-size="12" font-weight="bold" fill="#2C3C28" text-anchor="middle">W8x31</text>
    <text x="150" y="215" font-family="Arial" font-size="8" fill="#2C3C28" text-anchor="middle">ASTM A992</text>
    <text x="150" y="225" font-family="Arial" font-size="8" fill="#2C3C28" text-anchor="middle">AISC CERTIFIED</text>
  </g>
  
  <!-- Product information */
  <rect x="30" y="30" width="240" height="80" fill="white" stroke="#ddd" stroke-width="2" rx="8"/>
  <text x="150" y="55" font-family="Arial" font-size="18" font-weight="bold" fill="#2C3C28" text-anchor="middle">Structural Steel Beam</text>
  <text x="150" y="75" font-family="Arial" font-size="14" fill="#629D23" text-anchor="middle">W8x31 x 20 ft Length</text>
  <text x="150" y="90" font-family="Arial" font-size="12" fill="#666" text-anchor="middle">ASTM A992 â€¢ AISC Certified</text>
  <text x="150" y="102" font-family="Arial" font-size="10" fill="#666" text-anchor="middle">31 lbs per linear foot</text>
  
  <!-- Dimensions diagram -->
  <g stroke="#2C3C28" stroke-width="1" fill="none">
    <!-- Dimension lines -->
    <line x1="65" y1="120" x2="65" y2="280"/>
    <line x1="60" y1="125" x2="70" y2="125"/>
    <line x1="60" y1="275" x2="70" y2="275"/>
    
    <!-- Dimension text -->
    <text x="55" y="205" font-family="Arial" font-size="10" fill="#2C3C28" text-anchor="middle" transform="rotate(-90 55 205)">8.00"</text>
    
    <!-- Width dimension -->
    <line x1="70" y1="115" x2="230" y2="115"/>
    <line x1="75" y1="110" x2="75" y2="120"/>
    <line x1="225" y1="110" x2="225" y2="120"/>
    <text x="150" y="108" font-family="Arial" font-size="10" fill="#2C3C28" text-anchor="middle">8.00"</text>
  </g>
  
  <!-- Specifications box -->
  <rect x="40" y="350" width="220" height="45" fill="#f8f9fa" stroke="#e5e5e5" stroke-width="1" rx="5"/>
  <text x="60" y="367" font-family="Arial" font-size="10" font-weight="bold" fill="#2C3C28">Specifications:</text>
  <text x="60" y="378" font-family="Arial" font-size="9" fill="#333">Depth: 8.00" | Flange Width: 8.00" | Weight: 31 lbs/ft</text>
  <text x="60" y="388" font-family="Arial" font-size="9" fill="#333">Length: 20 feet | Grade: ASTM A992 High Strength Steel</text>
  
  <!-- Price -->
  <circle cx="80" cy="365" r="25" fill="#2C3C28"/>
  <text x="80" y="361" font-family="Arial" font-size="11" font-weight="bold" fill="white" text-anchor="middle">$425.00</text>
  <text x="80" y="372" font-family="Arial" font-size="8" fill="white" text-anchor="middle">per piece</text>
  
  <!-- Brand -->
  <rect x="180" y="380" width="60" height="18" fill="#629D23" rx="3"/>
  <text x="210" y="391" font-family="Arial" font-size="10" font-weight="bold" fill="white" text-anchor="middle">Nucor Steel</text>
  
  <!-- AISC certification -->
  <circle cx="240" cy="365" r="18" fill="#FFE884" stroke="#2C3C28" stroke-width="1"/>
  <text x="240" y="361" font-family="Arial" font-size="7" font-weight="bold" fill="#2C3C28" text-anchor="middle">AISC</text>
  <text x="240" y="370" font-family="Arial" font-size="6" fill="#2C3C28" text-anchor="middle">CERTIFIED</text>
</svg>
